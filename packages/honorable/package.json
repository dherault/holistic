{
  "name": "honorable",
  "version": "0.18.0",
  "description": "An extensible React component librarie for 2022 and beyond",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run clean:peerDependencies && npm run clean:build && nodemon --watch src --ext ts,tsx --exec \"npm run build\"",
    "build": "npx tsc --declaration",
    "clean": "npm run clean:build && npm run clean:node-modules && npm i && npm run clean:peerDependencies",
    "clean:node-modules": "rimraf node_module package-lock.json",
    "clean:peerDependencies": "rimraf node_modules/react node_modules/react-dom",
    "clean:build": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "\"git+https://github.com/dherault/honorable.git"
  },
  "keywords": [
    "React",
    "components",
    "librarie"
  ],
  "author": "David HÃ©rault <dherault@gmail.com> (https://github.com/dherault)",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@emotion/is-prop-valid": "^1.1.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "flexpad": "^3.0.0",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "nodemon": "^2.0.15",
    "normalize.css": "^8.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  }
}
