{
  "name": "honorable",
  "version": "0.87.0",
  "description": "An extensible React component librarie for 2022 and beyond",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "nodemon --watch src --watch tests --ext ts,tsx --exec \"npm run test\"",
    "dev": "npm run clean:peerDependencies && npm run clean:build && nodemon --watch src --ext ts,tsx --exec \"npm run build\"",
    "build": "npx tsc --declaration",
    "clean": "npm run clean:build && npm run clean:node-modules && npm i --force && npm run clean:peerDependencies",
    "clean:node-modules": "rimraf node_module package-lock.json",
    "clean:peerDependencies": "rimraf node_modules/react node_modules/react-dom",
    "clean:build": "rimraf dist",
    "prepublishOnly": "npm run clean && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "\"git+https://github.com/dherault/honorable.git"
  },
  "bugs": {
    "url": "https://github.com/dherault/honorable/issues"
  },
  "homepage": "https://github.com/dherault/honorable#readme",
  "keywords": [
    "React",
    "components",
    "librarie"
  ],
  "author": "David HÃ©rault <dherault@gmail.com> (https://github.com/dherault)",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "@emotion/is-prop-valid": "^1.1.2",
    "@floating-ui/react-dom": "^0.7.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "prop-types": "^15.8.1",
    "react-textarea-autosize": "^8.3.3",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "@emotion/react": ">=10.0.0",
    "@emotion/styled": ">=10.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.1",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-transition-group": "^4.4.4",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
